{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "defaultProject": "ng-blog",
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "css",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "css",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "css"
    }
  },
  "projects": {
    "ng-blog": {
      "projectType": "application",
      "root": "apps/ng-blog",
      "sourceRoot": "apps/ng-blog/src",
      "prefix": "blog",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/ng-blog/browser",
            "index": "apps/ng-blog/src/index.html",
            "main": "apps/ng-blog/src/main.ts",
            "polyfills": "apps/ng-blog/src/polyfills.ts",
            "tsConfig": "apps/ng-blog/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/ng-blog/src/favicon.ico",
              "apps/ng-blog/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/pink-bluegrey.css",
              "apps/ng-blog/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/ng-blog/src/environments/environment.ts",
                  "with": "apps/ng-blog/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ng-blog:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ng-blog:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ng-blog:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/ng-blog/src/**/*.ts",
              "apps/ng-blog/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/ng-blog"],
          "options": {
            "jestConfig": "apps/ng-blog/jest.config.js",
            "passWithNoTests": true
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist/ng-blog/server",
            "main": "apps/ng-blog/server.ts",
            "tsConfig": "apps/ng-blog/tsconfig.server.json"
          },
          "configurations": {
            "production": {
              "outputHashing": "media",
              "fileReplacements": [
                {
                  "replace": "apps/ng-blog/src/environments/environment.ts",
                  "with": "apps/ng-blog/src/environments/environment.prod.ts"
                }
              ],
              "sourceMap": false,
              "optimization": true
            }
          }
        },
        "serve-ssr": {
          "builder": "@nguniversal/builders:ssr-dev-server",
          "options": {
            "browserTarget": "ng-blog:build",
            "serverTarget": "ng-blog:server"
          },
          "configurations": {
            "production": {
              "browserTarget": "ng-blog:build:production",
              "serverTarget": "ng-blog:server:production"
            }
          }
        },
        "prerender": {
          "builder": "@nguniversal/builders:prerender",
          "options": {
            "browserTarget": "ng-blog:build:production",
            "serverTarget": "ng-blog:server:production",
            "routes": ["/"]
          },
          "configurations": {
            "production": {}
          }
        }
      }
    },
    "ng-blog-e2e": {
      "root": "apps/ng-blog-e2e",
      "sourceRoot": "apps/ng-blog-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/ng-blog-e2e/cypress.json",
            "tsConfig": "apps/ng-blog-e2e/tsconfig.e2e.json",
            "devServerTarget": "ng-blog:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ng-blog:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/ng-blog-e2e/**/*.{js,ts}"]
          }
        }
      }
    }
  }
}
